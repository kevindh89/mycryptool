<table class="table table-striped table-dark">
    <thead>
    <tr>
        <th scope="col">Product</th>
        <th scope="col">Size</th>
        <th scope="col">Price</th>
        <th scope="col">Date</th>
    </tr>
    </thead>
    <tbody>
    {% for trade in trades %}
        <tr>
            <td>{{ trade.productId }}</td>
            <td>{{ trade.size }}</td>
            <td>{{ trade.price }}</td>
            <td>{{ trade.tradeCreatedAt | date('d-m-Y H:i:s') }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>